{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "framework": null,
  "buildCommand": "docship --config docship.mjs --indir www",
  "installCommand": "npm i -g docship",
  "outputDirectory": "output",
  "cleanUrls": true,
  "trailingSlash": false,
  "github": {
    "silent": true
  },
  "rewrites": [
    {
      "source": "/1000os(.*)",
      "destination": "https://operating-system-in-1000-lines.vercel.app$1"
    }
  ]
}